  <script>
    
    var appURL = '<%=appURL %>'
    function getAppUrl(url = '') {
      if (url) {
        url += url.endsWith("/") ? "" : "/"
        return `${appURL}${url}`
      }
      return appURL
    }
    
  </script>
  <style>
    .focus:hover {
      fill: #007bff;
      cursor: pointer;
    }

    .focus:hover {
      transform: scale(1.1);
      transition: transform 0.2s ease;
    }

    .focus:focus {
      fill: #007bff;
      transform: scale(1.1);
    }

    .sortIcon.light {
      color: #ccc;
    }

    .dark {
      color: #333;
    }

    .datatable-top,
    .datatable-bottom {
      padding: 8px 0px;
    }

    .imageSizing {
      width: 50px; 
      height: 50px; 
      object-fit: cover; 
    }

    .cursor {
      cursor: pointer;
    }

    .removeImage {
      font-size: 12px; 
      padding: 2px 4px; 
      line-height: 1; 
    }

    .hidden {
      display: none;
    }

    .smallBox {
      width: 10px;
      height: 6px;
    }

    .clr1 {
      background-color: green;
    }

    .clr2 {
      background-color: red;
    }

    .clr3 {
      background-color: orange;
    }

    .clr4 {
      background-color: #adb5bd;
    }

